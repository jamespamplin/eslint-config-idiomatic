{
  "env": {
    "node": true,
    "mocha": true
  },
  "rules": {
    "no-undef": [2],
    "no-unused-vars": [2],
    "no-dupe-args": [2],
    "no-dupe-keys": [2],
    "no-duplicate-case": [2],
    "no-empty-character-class": [2],
    "no-empty": [2],
    "no-ex-assign": [2],
    "no-extra-boolean-cast": [2],
    "no-extra-semi": [2],
    "no-func-assign": [2],
    "no-inner-declarations": [2],
    "no-invalid-regexp": [2],
    "no-irregular-whitespace": [2],
    "no-negated-in-lhs": [2],
    "no-obj-calls": [2],
    "no-regex-spaces": [2],
    "no-sparse-arrays": [2],
    "no-unreachable": [2],
    "use-isnan": [2],
    "valid-typeof": [2],
    "no-redeclare": [2]
  }
}
